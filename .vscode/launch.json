  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "envFile": "${input:dotEnvFilePath}",
      "preLaunchTask": "my script task"
    }
],
"inputs": [
    {
        "id": "dotEnvFilePath",
        "type": "command",
        "command": "azure-dev.commands.getDotEnvFilePath"
    }
]
}
